{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "paths": {
      "@family-task/shared": [
        "./src/db/shared/index.ts"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}